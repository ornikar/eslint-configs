{
  "name": "@ornikar/eslint-configs",
  "private": true,
  "repository": "ornikar/eslint-configs",
  "scripts": {
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --quiet --report-unused-disable-directives .",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "$(yarn bin ornikar-prettier-check)",
    "lint:prettier:fix": "$(yarn bin ornikar-prettier-fix)",
    "postinstall": "ornikar-repo-config-postinstall",
    "release": "lerna publish --conventional-commits",
    "version": "YARN_ENABLE_IMMUTABLE_INSTALLS=false yarn && git add yarn.lock"
  },
  "prettier": "@ornikar/prettier-config",
  "packageManager": "yarn@3.3.1",
  "engines": {
    "node": ">=16.10.0"
  },
  "commitlint": {
    "extends": [
      "@ornikar/commitlint-config/monorepo"
    ]
  },
  "workspaces": [
    "@ornikar/*"
  ],
  "devDependencies": {
    "@ornikar/commitlint-config": "5.0.1",
    "@ornikar/lerna-config": "9.0.1",
    "@ornikar/prettier-config": "6.0.1",
    "@ornikar/repo-config": "10.3.1",
    "check-package-dependencies": "3.10.0",
    "eslint": "8.29.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-unicorn": "45.0.2",
    "prettier": "2.8.1"
  }
}
