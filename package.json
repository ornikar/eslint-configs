{
  "name": "@ornikar/eslint-configs",
  "private": true,
  "repository": "ornikar/eslint-configs",
  "scripts": {
    "version": "YARN_ENABLE_IMMUTABLE_INSTALLS=false yarn && git add yarn.lock",
    "postinstall": "ornikar-repo-config-postinstall",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --quiet --report-unused-disable-directives .",
    "release": "lerna publish --conventional-commits"
  },
  "prettier": "@ornikar/prettier-config",
  "packageManager": "yarn@3.2.2",
  "engines": {
    "node": ">=16.15.1"
  },
  "commitlint": {
    "extends": [
      "@ornikar/commitlint-config"
    ]
  },
  "workspaces": [
    "@ornikar/*"
  ],
  "devDependencies": {
    "@ornikar/commitlint-config": "5.0.0",
    "@ornikar/lerna-config": "7.0.0",
    "@ornikar/prettier-config": "6.0.0",
    "@ornikar/repo-config": "10.0.0",
    "check-package-dependencies": "3.9.2",
    "eslint": "8.22.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-unicorn": "43.0.2",
    "lerna": "5.1.8",
    "prettier": "2.7.1"
  }
}
