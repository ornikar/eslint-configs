{
  "name": "@ornikar/eslint-configs",
  "private": true,
  "repository": "ornikar/eslint-configs",
  "scripts": {
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --quiet --report-unused-disable-directives .",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "prettier --check . '**/.env*' '**/*.env'",
    "lint:prettier:fix": "prettier --check . '**/.env*' '**/*.env'",
    "postinstall": "ornikar-repo-config-postinstall",
    "release": "lerna publish --conventional-commits",
    "version": "YARN_ENABLE_IMMUTABLE_INSTALLS=false yarn && git add yarn.lock"
  },
  "prettier": "@ornikar/prettier-config",
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=16.10.0"
  },
  "commitlint": {
    "extends": [
      "@ornikar/commitlint-config/monorepo"
    ]
  },
  "workspaces": [
    "@ornikar/*"
  ],
  "devDependencies": {
    "@ornikar/commitlint-config": "5.0.2",
    "@ornikar/lerna-config": "9.1.0",
    "@ornikar/prettier-config": "6.0.6",
    "@ornikar/repo-config": "10.4.5",
    "check-package-dependencies": "6.3.1",
    "eslint": "8.38.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-unicorn": "46.0.0",
    "prettier": "2.8.7"
  }
}
