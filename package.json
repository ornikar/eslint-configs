{
  "name": "@ornikar/eslint-configs",
  "private": true,
  "repository": "ornikar/eslint-configs",
  "scripts": {
    "version": "YARN_ENABLE_IMMUTABLE_INSTALLS=false yarn && git add yarn.lock",
    "postinstall": "ornikar-repo-config-postinstall",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --quiet --report-unused-disable-directives .",
    "release": "lerna publish --conventional-commits"
  },
  "prettier": "@ornikar/prettier-config",
  "engines": {
    "node": ">=14.17.5",
    "yarn": ">=1.10.1"
  },
  "commitlint": {
    "extends": [
      "@ornikar/commitlint-config"
    ]
  },
  "workspaces": [
    "@ornikar/*"
  ],
  "devDependencies": {
    "@ornikar/commitlint-config": "4.0.3",
    "@ornikar/lerna-config": "6.3.4",
    "@ornikar/prettier-config": "4.0.4",
    "@ornikar/repo-config": "8.2.0",
    "check-package-dependencies": "3.5.2",
    "eslint": "8.11.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "39.0.0",
    "lerna": "4.0.0",
    "prettier": "2.6.2"
  },
  "packageManager": "yarn@3.2.0"
}
